{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb",
      "prettier",
      "plugin:react/jsx-runtime"
     
    ],
    // "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react","prettier"],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prop-types": 0,
      "react/destructuring-assignment": [0]
    //   "prettier/prettier": "error"
    },
    "settings": {
        "import/resolver": {
          "node": {          
            "paths": ["./src"],
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
          }
        }
    }
  }